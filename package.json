{
  "name": "hexo-reference",
  "version": "1.0.4",
  "description": "A plugin to support markdown footnotes and Wiki-Style tooltip reference in your Hexo blog posts.",
  "main": "index.js",
  "scripts": {
    "mocha": "./node_modules/.bin/mocha -u bdd",
    "cov": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whyliam/hexo-reference"
  },
  "keywords": [
    "hexo",
    "plugin",
    "footnote",
    "footnotes",
    "reference",
    "tooltip"
  ],
  "author": "xiongziq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whyliam/hexo-reference/issues"
  },
  "homepage": "https://github.com/whyliam/hexo-reference#readme",
  "devDependencies": {
    "chai": "^5.2.0",
    "coveralls": "^3.1.1",
    "eslint-config-google": "^0.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^11.1.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "hexo-util": "^3.3.0"
  }
}
